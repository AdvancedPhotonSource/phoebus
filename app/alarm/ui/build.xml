<project default="module-alarm-ui">
  <import file="../../../dependencies/ant_settings.xml"/>

  <target name="module-alarm-ui">
    <mkdir dir="${classes}"/>
    <javac destdir="${classes}">
      <src path="${src}"/>
      <classpath>
        <path refid="app-classpath"/>
        <pathelement path="../model/${build}/alarm-model-${version}.jar"/>
      </classpath>
    </javac>
  	
    <jar destfile="${build}/alarm-ui-${version}.jar">
      <fileset dir="${classes}"/>
      <fileset dir="${resources}"/>
    </jar>
  </target>
	
</project>